<div class="index">
  <h2 class="index_header">Ticket List:</h2>
  <div class="table">
    <div class="table_header">
      <h3>Ticket Id:</h3>
      <h3>Created At(UTC):</h3>
    </div>
    <ul>
      <% @tickets.each do |ticket| %>
        <li class="ticket">
          <%= link_to ticket_path(ticket) do %>
            <div class="ticket_id">
              <%= ticket.id %>
            </div>
            <div class="ticket_date">
              <%= ticket.created_at.strftime("%m-%d-%Y, %H:%M:%S") %>:
            </div>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
