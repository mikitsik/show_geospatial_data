<div class="index">
  <div class="index_header">Ticket List</div>
  <div class="table">
    <div class="table_header">
      <div>Ticket Id:</div>
      <div>Created At(UTC):</div>
    </div>
    <ul>
      <% @tickets.each do |ticket| %>
        <li class="ticket">
          <%= link_to ticket_path(ticket), class: "ticket_link" do %>
            <div class="ticket_id">
              <%= ticket.id %>
            </div>
            <div class="ticket_date">
              <%= ticket.created_at.strftime("%m-%d-%Y, %H:%M:%S") %>
            </div>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
